alias ducks='du -cksh * | sort -hr'
if [ "$TERM" = "xterm-kitty" ]; then
  alias icat="kitty +kitten icat"
  alias ssh="kitty +kitten ssh"
  else
    alias icat="echo 'icat is a kitty terminal command'"
fi
